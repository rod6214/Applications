@model IEnumerable<web01.Models.Usuario>
@{

    ViewBag.Title = "Inicio";
}

<div class="row mt-6">
    <h3 class="text-center col"><span class="border-bottom"> Usuarios</span></h3>
</div>

<div class="row justify-content-md-center">
    <table class="table col-md-6 table-bordered table-hover table-striped">
        <thead>
            <tr>
                <th scope="col">id</th>
                <th scope="col">Nombre</th>
                <th scope="col">apellido</th>
                <th scope="col">Correo</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var mod in Model)
            {
                <tr>
                    <th scope="row">@mod.id</th>
                    <td>@mod.nombre</td>
                    <td>@mod.apellido</td>
                    <td>@mod.correo</td>
                </tr>
            }
        </tbody>
    </table>
</div>